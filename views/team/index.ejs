<div class="container">
<h1 style="text-align:center">Registered Teams</h1>
    <% teams.forEach( item => { %>
        <button class="btn btn-outline-info btn-lg btn-block" style="background-color: lightskyblue;">
            <h2><%= item.name%></h2>
            <p>Number of players: <%= item.player_count%></p>
            <p>Tier List: <%= item.tier%></p>   
        </button>

        <button class="btn btn-outline-info btn-lg btn-block" style="background-color: lightskyblue;">
            <h2><%= item.name%></h2>
            <p>Number of players: <%= item.player_count%></p>
            <p>Tier List: <%= item.tier%></p>  
        </button>

        <button class="btn btn-outline-info btn-lg btn-block" style="background-color: lightskyblue;">
            <h2><%= item.name%></h2>
            <p>Number of players: <%= item.player_count%></p>
            <p>Tier List: <%= item.tier%></p>  
        </button>
       
    <%})%>
</div>
